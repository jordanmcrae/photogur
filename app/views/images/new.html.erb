<h1>Add an Image</h1>

<%= link_to "Back to Images", images_url %>

<%= form_for @image do |f| %>
  <p>
    <%= f.label :artist %><br>
    <%= f.text_field :artist %>
  </p>

  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.label :url %><br>
    <%= f.text_field :url %>
  </p>

  <p>
    <%= f.submit "Submit" %>
  </p>

<% end %>